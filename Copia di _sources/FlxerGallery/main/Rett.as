package FlxerGallery.main{	import flash.text.* 	import flash.text.TextField;	import flash.display.DisplayObject;    import flash.display.Graphics;    import flash.display.Sprite;	public class Rett extends Sprite {		public function Rett(xx,yy,w,h,col,o_col,aa) {			x=xx;			y=yy;			if (o_col != null) {				graphics.lineStyle(0,o_col,aa);			}			if (col != null) {				graphics.beginFill(col,aa);			}			graphics.moveTo(0,0);			graphics.lineTo(w,0);			graphics.lineTo(w,h);			graphics.lineTo(0,h);			graphics.lineTo(0,0);		}	}}